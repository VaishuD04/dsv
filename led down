 
Source Code: 
 
#define LED1 6 
#define LED2 7  
#define LED3 8 
#define LED4 9 
 
void setup(){ 
pinMode(LED1, OUTPUT); 
pinMode(LED2, OUTPUT); 
pinMode(LED3, OUTPUT); 
pinMode(LED4, OUTPUT); 
} 
 
void loop(){ 
digitalWrite(LED1,LOW); 
digitalWrite(LED2,LOW); 
digitalWrite(LED3,LOW); 
digitalWrite(LED4,LOW); 
delay(1000); 
 
for(int i = 6; i <= 9; i++){ 
                  digitalWrite(i, HIGH); 
                  delay(1000); 
  } 
}
